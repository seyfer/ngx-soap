{
  "name": "ngx-soap-next",
  "version": "0.18.1",
  "description": "SOAP service for Angular",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "typings": "src/index.d.ts",
  "moduleName": "ngx-soap",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development --proxy-config proxy.conf.json",
    "serve": "ng serve --configuration development --proxy-config proxy.conf.json",
    "build": "ng build --configuration production ngx-soap-app",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:lib": "jest --testPathPattern=projects/ngx-soap",
    "test:app": "jest --testPathPattern=src",
    "build:lib": "ng build --configuration production ngx-soap && cp README.md dist/ngx-soap && cp LICENSE dist/ngx-soap",
    "build:all": "npm run build:lib && npm run build",
    "pack": "cd ./dist/ngx-soap/ && npm pack && cd ../.. && cp dist/ngx-soap/ngx*tgz ..",
    "publish": "cd ./dist/ngx-soap/ && npm publish",
    "publish:dry-run": "cd ./dist/ngx-soap/ && npm publish --dry-run",
    "build:lib:publish": "npm run test && npm run build:lib && npm run pack && npm run publish",
    "build:lib:publish:dry-run": "npm run test && npm run build:lib && npm run pack && npm run publish:dry-run",
    "bump:patch": "npm version patch --no-git-tag-version",
    "bump:minor": "npm version minor --no-git-tag-version",
    "bump:major": "npm version major --no-git-tag-version"
  },
  "keywords": [
    "Angular",
    "Angular18",
    "@angular",
    "SOAP",
    "WSDL"
  ],
  "author": "Luca Lulani, marcward, Oleg Abrazhaev",
  "license": "MIT",
  "readme": "README.md",
  "homepage": "https://github.com/seyfer/ngx-soap",
  "bugs": {
    "url": "https://github.com/seyfer/ngx-soap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seyfer/ngx-soap.git"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular/animations": "^18.2.14",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.14",
    "@angular/compiler": "^18.2.14",
    "@angular/compiler-cli": "^18.2.14",
    "@angular/core": "^18.2.14",
    "@angular/forms": "^18.2.14",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.14",
    "@angular/platform-browser-dynamic": "^18.2.14",
    "@angular/router": "^18.2.14",
    "assert": "^2.1.0",
    "bluebird": "^3.7.2",
    "buffer": "^5.1.0",
    "concat-stream": "^1.6.2",
    "core-js": "^3.47.0",
    "crypto-js": "^4.2.0",
    "debug": "^4.4.3",
    "events": "^3.0.0",
    "httpntlm": "^1.7.6",
    "lodash": "^4.17.21",
    "process": "^0.11.10",
    "rxjs": "^7.8.2",
    "sax": "^1.4.1",
    "stream": "0.0.2",
    "ts-evaluator": "^1.2.0",
    "tslib": "^2.8.1",
    "url": "^0.11.4",
    "xml-crypto": "^6.1.2",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular/cli": "^18.2.21",
    "@angular/language-service": "^18.2.14",
    "@types/estree": "^1.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.19.130",
    "autoprefixer": "^10.4.22",
    "codelyzer": "^6.0.2",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.57.1",
    "global": "^4.4.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "ng-packagr": "^18.2.1",
    "postcss": "^8.5.6",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.5",
    "typescript": "~5.4.5",
    "vm-browserify": "^1.1.2"
  }
}
