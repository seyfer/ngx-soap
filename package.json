{
  "name": "ngx-soap-next",
  "version": "0.19.0",
  "description": "SOAP service for Angular",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "typings": "src/index.d.ts",
  "moduleName": "ngx-soap",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development --proxy-config proxy.conf.json",
    "serve": "ng serve --configuration development --proxy-config proxy.conf.json",
    "build": "ng build --configuration production ngx-soap-app",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:lib": "jest --testPathPattern=projects/ngx-soap",
    "test:app": "jest --testPathPattern=src",
    "build:lib": "ng build --configuration production ngx-soap && cp README.md dist/ngx-soap && cp LICENSE dist/ngx-soap",
    "build:all": "npm run build:lib && npm run build",
    "pack": "cd ./dist/ngx-soap/ && npm pack && cd ../.. && cp dist/ngx-soap/ngx*tgz ..",
    "publish": "cd ./dist/ngx-soap/ && npm publish",
    "publish:dry-run": "cd ./dist/ngx-soap/ && npm publish --dry-run",
    "build:lib:publish": "npm run test && npm run build:lib && npm run pack && npm run publish",
    "build:lib:publish:dry-run": "npm run test && npm run build:lib && npm run pack && npm run publish:dry-run",
    "bump:patch": "npm version patch --no-git-tag-version",
    "bump:minor": "npm version minor --no-git-tag-version",
    "bump:major": "npm version major --no-git-tag-version"
  },
  "keywords": [
    "Angular",
    "Angular19",
    "@angular",
    "SOAP",
    "WSDL"
  ],
  "author": "Luca Lulani, marcward, Oleg Abrazhaev",
  "license": "MIT",
  "readme": "README.md",
  "homepage": "https://github.com/seyfer/ngx-soap",
  "bugs": {
    "url": "https://github.com/seyfer/ngx-soap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seyfer/ngx-soap.git"
  },
  "dependencies": {
    "@angular/animations": "^20.3.14",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^20.3.14",
    "@angular/compiler": "^20.3.14",
    "@angular/compiler-cli": "^20.3.14",
    "@angular/core": "^20.3.14",
    "@angular/forms": "^20.3.14",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^20.3.14",
    "@angular/platform-browser-dynamic": "^20.3.14",
    "@angular/router": "^20.3.14",
    "assert": "^2.1.0",
    "bluebird": "^3.7.2",
    "buffer": "^5.1.0",
    "concat-stream": "^1.6.2",
    "core-js": "^2.5.4",
    "crypto-js": "^4.2.0",
    "debug": "^4.4.3",
    "events": "^3.0.0",
    "httpntlm": "^1.7.6",
    "lodash": "^4.17.21",
    "process": "^0.11.10",
    "rxjs": "^7.8.2",
    "sax": "^1.4.1",
    "stream": "^0.0.3",
    "ts-evaluator": "^1.2.0",
    "tslib": "^2.8.1",
    "url": "^0.11.4",
    "xml-crypto": "^6.1.2",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.1",
    "@angular/build": "^20.3.12",
    "@angular/cli": "^20.3.12",
    "@angular/language-service": "^20.3.14",
    "@types/estree": "^1.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "autoprefixer": "^10.4.22",
    "codelyzer": "^6.0.2",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.57.1",
    "global": "^4.4.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^15.0.3",
    "ng-packagr": "^20.3.2",
    "postcss": "^8.5.6",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.5",
    "typescript": "~5.8.3",
    "vm-browserify": "^1.1.2"
  }
}
