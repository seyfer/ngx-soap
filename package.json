{
  "name": "ngx-soap-next",
  "version": "0.17.0",
  "description": "SOAP service for Angular",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "typings": "src/index.d.ts",
  "moduleName": "ngx-soap",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development --proxy-config proxy.conf.json",
    "serve": "ng serve --configuration development --proxy-config proxy.conf.json",
    "build": "ng build --configuration production ngx-soap-app",
    "test": "ng test",
    "test:e2e": "ng e2e",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:lib": "ng test ngx-soap",
    "test:lib:headless": "ng test ngx-soap --watch=false --browsers=ChromeHeadless",
    "test:all:headless": "npm run test:headless && npm run test:lib:headless",
    "build:lib": "ng build --configuration production ngx-soap && cp README.md dist/ngx-soap && cp LICENSE dist/ngx-soap",
    "pack": "cd ./dist/ngx-soap/ && npm pack && cd ../.. && cp dist/ngx-soap/ngx*tgz ..",
    "publish": "cd ./dist/ngx-soap/ && npm publish",
    "publish:dry-run": "cd ./dist/ngx-soap/ && npm publish --dry-run",
    "build:lib:publish": "npm run test:lib:headless && npm run build:lib && npm run pack && npm run publish",
    "bump:patch": "npm version patch --no-git-tag-version",
    "bump:minor": "npm version minor --no-git-tag-version",
    "bump:major": "npm version major --no-git-tag-version"
  },
  "keywords": [
    "Angular",
    "Angular17",
    "@angular",
    "SOAP",
    "WSDL"
  ],
  "author": "Luca Lulani, marcward, Oleg Abrazhaev",
  "license": "MIT",
  "readme": "README.md",
  "homepage": "https://github.com/seyfer/ngx-soap",
  "bugs": {
    "url": "https://github.com/seyfer/ngx-soap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seyfer/ngx-soap.git"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "assert": "^2.1.0",
    "bluebird": "^3.7.2",
    "buffer": "^5.1.0",
    "concat-stream": "^1.6.2",
    "core-js": "^2.5.4",
    "crypto-js": "^4.2.0",
    "eslint": "^8.52.0",
    "events": "^3.0.0",
    "httpntlm": "^1.7.6",
    "lodash": "^4.17.10",
    "process": "^0.11.10",
    "rxjs": "^7.8.1",
    "sax": "^1.2.4",
    "stream": "0.0.2",
    "ts-evaluator": "^1.2.0",
    "tslib": "^2.6.2",
    "url": "^0.11.3",
    "uuid": "^3.3.2",
    "xml-crypto": "^1.5.3",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular/cli": "^17.3.17",
    "@angular/language-service": "^17.3.12",
    "@types/estree": "^1.0.5",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.18.71",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "ng-packagr": "^17.3.0",
    "ts-node": "~5.0.1",
    "typescript": "~5.2.2"
  }
}
